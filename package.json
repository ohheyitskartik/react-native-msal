{
  "name": "react-native-msal",
  "version": "4.0.4",
  "description": "React Native wrapper for iOS and Android",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "source": "src/index",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --cache --fix"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "msal",
    "azure",
    "b2c",
    "aad"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elkinjosetm/react-native-msal.git"
  },
  "author": "Elkin Torres <elkinjosetm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elkinjosetm/react-native-msal/issues"
  },
  "homepage": "https://github.com/elkinjosetm/react-native-msal#readme",
  "dependencies": {
    "@azure/msal-browser": "^2.1.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.5",
    "@types/react-native": "^0.66.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-native": "^0.64.1",
    "semantic-release": "^19.0.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 120
  }
}
